#!/bin/bash

echo "=========================================================================================="
echo "ARGS: ${@}"
echo "=========================================================================================="
echo "Output of 'env'"
echo "---------------"
env
echo "=========================================================================================="
echo ".env"
echo "----"
if [ -f .env ]; then
  cat .env
else
  echo "(doesn't exist)"
fi
echo "=========================================================================================="

cd /app
/usr/local/bin/shoreman
